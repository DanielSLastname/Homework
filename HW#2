#Homework #2
#Question #1
import numpy as np
from numpy import linalg as linalg
a = np.matrix ('1  0  ;  0 1')
print(a)

w , v = linalg.eig(a)

#w is the eigen values
#v is the corresponding eigen vectors

print(w)
print(v)


#Question #2
arr = np.array([[0, 0, 0], [2, 0, 0 ],[4, 0, 2], [3, 2, 0 ]],dtype=np.float32)
  
print(arr)
  
U, s, V = np.linalg.svd(arr, full_matrices=False)

print("\nU=", U, "\n\ns=", s, "\n\nV=", V)

#Question #3

a = np.array([[1, 2], [3, 4]])
np.linalg.det(a)
